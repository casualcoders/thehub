{
  "name": "thehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "js:prettier": "prettier --write '**/*.js'",
    "js:lint": "eslint '**/*.js' --cache",
    "styles:prettier": "prettier --write '**/*.scss'",
    "styles:lint": "stylelint '**/**.*.(scss|css)'",
    "styles:lint:check": "stylelint-config-prettier-check",
    "prettier:all": "concurrently 'npm:js:prettier' 'npm:styles:prettier'",
    "testAndLint": "concurrently 'npm:test' 'npm:js:lint' 'npm:styles:lint'"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0"
  }
}
